{
  "name": "inventory-service",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node -r tsconfig-paths/register dist/main.js",
    "start:dev": "ts-node -r tsconfig-paths/register src/main.ts",
    "dev": "nodemon --watch src --ext ts,graphql --exec ts-node -r tsconfig-paths/register src/main.ts",
    "build": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@apollo/subgraph": "^2.9.0",
    "@nestjs/apollo": "^12.1.1",
    "@nestjs/common": "^10.4.20",
    "@nestjs/core": "^10.4.20",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/platform-express": "^10.4.20",
    "apollo-server-express": "^3.12.1",
    "express": "^4.21.2",
    "graphql": "^16.9.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.2",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
